<head>
    <meta charset="UTF-8"/>
    <title>{{title}}</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.0/font/bootstrap-icons.css">
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-mousewheel/3.1.13/jquery.mousewheel.min.js"></script>
    <script src="../javascripts/mousewheel.js"></script>
    <script>
    function switchp1p2() {
      var p1 = document.getElementById("Player1").value;
      var p2 = document.getElementById("Player2").value;
      document.getElementById("Player1").value=p2;
      document.getElementById("Player2").value=p1;
    }
    function switchp3p4() {
      var p3 = document.getElementById("Player3").value;
      var p4 = document.getElementById("Player4").value;
      document.getElementById("Player3").value=p4;
      document.getElementById("Player4").value=p3;
    }
    </script>
</head>



<a href="http://tsgd:9000/">
    <img src="http://tsgd:9000/images/logo_white_150px.png" width="80" height="80"  >
</a>
<i style="color: #3dd5f3">Welcome to the {{title}}</i>
<br>
</img>
<br>

<nav class="navbar navbar-dark bg-dark">
    <button><a href="/register"  >register a Player</a></button>
    <button><a href="/charts"  >show games charts</a></button>
    <button><a href="/index"  >back</a></button>

</nav>



<div>


    <div class="card">
        <div class="card-body" style="background-image:url('../images/tisch_1080_la.png');background-repeat: no-repeat">
            <div class="row">
                <div class="col-sm">
                    <h3>reinsert the data of the same players:</h3>
                    <form action="/reinsert" method="post" >

                        <br>
                        <img src="http://tsgd:9000/images/ts_pro.png" width="100" height="100"  alt="">

                        <h3 style="color: #3dd5f3">Team 1</h3>

                        <div class="row">
                            <br>
                            <div class="col-sm">
                                <div class="form-group" >
                                    <label>
                                        <input list="Player1" name="Player1" placeholder="{{item1}}" style="background-color: #2c3034" id="Player1" class="form-control form-rounded" value="{{item1}}"/></label>

				</div>
				</div>
				<div class="row col-sm form-group">
				  <button type="button" id="player1_switch" onclick="switchp1p2(); return false;">switch players</button></div>
                            <div class="col-sm">
                                <div class="form-group">
                                    <label>
                                        <input list="Player2" name="Player2" placeholder="{{item2}}" style="background-color: #2c3034" id="Player2" class="form-control form-rounded" value="{{item2}}" /></label>

                                </div>
                            </div></div>
                        <br>
                        <img src="http://tsgd:9000/images/ts_pro.png" width="100" height="100"  alt="">

                        <h3 style="color: #3dd5f3">Team 2</h3>
                        <div class="row">
                            <br>
                            <div class="col-sm">

                                <div class="form-group" >
                                    <label>
                                        <input list="Player3" name="Player3" placeholder="{{item3}}"  style="background-color: #2c3034" id="Player3" class="form-control form-rounded" value="{{item3}}"/></label>

                                </div></div>
				<div class="row col-sm form-group">
				  <button type="button" id="player3_switch" onclick="switchp3p4(); return false;">switch players</button></div>

                            <div class="col-sm">
                                <div class="form-group">
                                    <label>
                                        <input list="Player4" name="Player4" placeholder="{{item4}}" style="background-color: #2c3034" id="Player4" class="form-control form-rounded" value="{{item4}}"/></label>

                                </div>
                            </div></div>
                        <br>
                        <svg xmlns="http://www.w3.org/2000/svg" width="50" height="50" fill="currentColor" class="bi bi-bookmark-star" viewBox="0 0 16 16" style="color: gold">
                            <path d="M7.84 4.1a.178.178 0 0 1 .32 0l.634 1.285a.178.178 0 0 0 .134.098l1.42.206c.145.021.204.2.098.303L9.42 6.993a.178.178 0 0 0-.051.158l.242 1.414a.178.178 0 0 1-.258.187l-1.27-.668a.178.178 0 0 0-.165 0l-1.27.668a.178.178 0 0 1-.257-.187l.242-1.414a.178.178 0 0 0-.05-.158l-1.03-1.001a.178.178 0 0 1 .098-.303l1.42-.206a.178.178 0 0 0 .134-.098L7.84 4.1z"/>
                            <path d="M2 2a2 2 0 0 1 2-2h8a2 2 0 0 1 2 2v13.5a.5.5 0 0 1-.777.416L8 13.101l-5.223 2.815A.5.5 0 0 1 2 15.5V2zm2-1a1 1 0 0 0-1 1v12.566l4.723-2.482a.5.5 0 0 1 .554 0L13 14.566V2a1 1 0 0 0-1-1H4z"/>
                        </svg>
                        <br>
                        <br>
                        <div class="row">
                            <div class="col-sm">
                                <div class="form-group">
                                    <input type="text" class="form-control" id="team1" placeholder="Team1-Points" onfocus="this.value = '1'" onblur="this.placeholder = 'Team1-Points'" onClick="this.select();" name="team1" style="background-color: #2c3034" class="form-control form-rounded">
                                </div></div>
                            <div class="col-sm">
                                <div class="form-group">
                                    <input type="text" class="form-control" id="team2" placeholder="Team2-Points" onfocus="this.value = '1'" onblur="this.placeholder = 'Team1-Points'" onClick="this.select();" name="team2" style="background-color: #2c3034" class="form-control form-rounded">
                                </div>
                            </div></div>
                        <br>
                        <button type="submit" class="btn btn-primary">reinsert data</button>
                        <br>
                    </form>
                    <br>
                </div>


            </div>

        </div> </div>




</div>




