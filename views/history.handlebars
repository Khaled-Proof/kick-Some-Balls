    <script type="text/javascript">

        window.onload = function () {
            var chart = new CanvasJS.Chart("chartContainer", {

                data: [
                    {
                        // Change type to "doughnut", "line", "splineArea", etc.
                        type: "doughnut",

                        dataPoints: [
                            { label: "{{test1}}",  y: {{won}},color: "green",indexLabel:"{{wonpresent}}%" },
                            { label: "{{test1}}", y: {{lost}},color: "red",indexLabel:"{{lospresent}}%"   }
                        ]
                    }
                ]
            });
            chart.render();
        }
    </script>
